<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        こんにちは、
        <%= link_to "#{@user.name}さん", user_path(current_user.id), class: :greeting__link %>
      </div>
    <% end %>
    <div class="card__wrapper">
       <% if @prototypes %>
        <%= render partial: 'prototype', locals: {prototypes: :@prototypes}%>
      <% end %>
    </div>
  </div>
</main>
